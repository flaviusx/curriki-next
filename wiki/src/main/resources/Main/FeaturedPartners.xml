#macro(display_partner $pname $pinfo $imagen $links)
#set($image = $xwiki.getAttachmentURL("Main.FeaturedPartners", $imagen))
#set($text = $pinfo)
#foreach($link in $links)
#set($lurl = $link.get(1))
#normalizelink($lurl)
#set($text = $text + '&lt;br/&gt;&lt;a href="' + ${lurl} + '"&gt;' + $link.get(0) + '&lt;/a&gt;')
#end
##
#subtitlebar($pname "" "" "blue")
#paragraphimg("logo" $image "" $text "" "" "")
&lt;div class="clr"&gt;&lt;/div&gt;
#end
